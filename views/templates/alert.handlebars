<table
	width="100%"
	border="0"
	align="center"
	cellpadding="0"
	cellspacing="0"
>
	<!-- START HEADER/BANNER -->
	<tbody>
		<tr>
			<td align="center">
				<table
					class="col-600"
					width="600"
					border="0"
					align="center"
					cellpadding="0"
					cellspacing="0"
				>
					<tbody>
						<tr>
							<td
								align="center"
								valign="top"
								background="https://designmodo.com/demo/emailtemplate/images/header-background.jpg"
								bgcolor="#66809b"
								style="background-size:cover; background-position:center;"
							>
								<table
									class="col-600"
									width="600"
									height="400"
									border="0"
									align="center"
									cellpadding="0"
									cellspacing="0"
								>
									<tbody>
										<tr>
											<td height="40"></td>
										</tr>
										<tr>
											<td
												align="center"
												style="font-family: 'Raleway', sans-serif; font-size:37px; color:#ffffff; line-height:24px; font-weight: bold; letter-spacing: 7px;"
											>
												Новая Заявка
											</td>
										</tr>
										<tr>
											<td
												align="left"
												style="font-family: 'Lato', sans-serif; font-size:15px; color:#ffffff; line-height:24px; font-weight: 300; padding:0 50px;"
											>
												Имя: {{name}}
											</td>
										</tr>
										<tr>
											<td
												align="left"
												style="font-family: 'Lato', sans-serif; font-size:15px; color:#ffffff; line-height:24px; font-weight: 300; padding:0 50px;"
											>
												Почта: {{email}}
											</td>
										</tr>
										<tr>
											<td
												align="left"
												style="font-family: 'Lato', sans-serif; font-size:15px; color:#ffffff; line-height:24px; font-weight: 300; padding:0 50px;"
											>
												Контактный номер: {{contact_number}}
											</td>
										</tr>
										<tr>
											<td
												align="left"
												style="font-family: 'Lato', sans-serif; font-size:15px; color:#ffffff; line-height:24px; font-weight: 300; padding:0 50px;"
											>
												Дата рождения: {{birthday}}
											</td>
										</tr>
										<tr>
											<td
												align="left"
												style="font-family: 'Lato', sans-serif; font-size:15px; color:#ffffff; line-height:24px; font-weight: 300; padding:0 50px;"
											>
												Страна/Город рождения: {{birth_location}}
											</td>
										</tr>
										<tr>
											<td
												align="left"
												style="font-family: 'Lato', sans-serif; font-size:15px; color:#ffffff; line-height:24px; font-weight: 300; padding:0 50px;"
											>
												Гражданство: {{citizenship}}
											</td>
										</tr>
										<tr>
											<td
												align="left"
												style="font-family: 'Lato', sans-serif; font-size:15px; color:#ffffff; line-height:24px; font-weight: 300; padding:0 50px;"
											>
												ПМЖ (страна/город): {{place_of_live}}
											</td>
										</tr>
										<tr>
											<td
												align="left"
												style="font-family: 'Lato', sans-serif; font-size:15px; color:#ffffff; line-height:24px; font-weight: 300; padding:0 50px;"
											>
												Рост/вес: {{height_weight}}
											</td>
										</tr>

										<tr>
											<td height="50"></td>
										</tr>
									</tbody>
								</table>
							</td>
						</tr>
					</tbody>
				</table>
			</td>
		</tr>

		<!-- END HEADER/BANNER -->
	</tbody>
</table>